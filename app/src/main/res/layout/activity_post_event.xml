<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".post_event">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/createevent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button14"
        android:layout_width="319dp"
        android:layout_height="51dp"
        android:background="#00FFFFFF"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="658dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="178dp"
        android:layout_height="47dp"
        android:background="#FFFFFF"
        android:text="Private?"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.273" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="281dp"
        android:layout_height="38dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="92dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="291dp"
        android:layout_height="41dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Description"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="152dp" />

    <EditText
        android:id="@+id/editTextTime"
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="time"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="274dp"
        tools:text="Time Slot 1" />

    <EditText
        android:id="@+id/editTextTime2"
        android:layout_width="291dp"
        android:layout_height="41dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="time"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="341dp"
        tools:text="Time Slot 2" />

    <EditText
        android:id="@+id/editTextTime3"
        android:layout_width="287dp"
        android:layout_height="40dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="time"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="408dp"
        tools:text="Time Slot 3" />

    <EditText
        android:id="@+id/editTextTime4"
        android:layout_width="286dp"
        android:layout_height="39dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="time"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="472dp"
        tools:text="Time Slot 4" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="289dp"
        android:layout_height="43dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="535dp"
        tools:text="Due Date" />

    <Button
        android:id="@+id/button20"
        android:layout_width="37dp"
        android:layout_height="48dp"
        android:background="#00FFFFFF"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="28dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="394dp"
        android:layout_height="87dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>